        "# Hide Drive notifications\n",
        "from google.colab import output\n",
        "output.eval_js('''\n",
        "    const notifications = document.querySelectorAll('[aria-label*=\"Drive\"], [class*=\"drive-warning\"]');\n",
        "    notifications.forEach(el => el.style.display = \"none\");\n",
        "''')"
